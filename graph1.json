{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "transform": [],
  "width": 500,
  "height": 500,
  "data": {
    "url": "https://raw.githubusercontent.com/thedevLEGEND27/data-viz-2.github.io/main/FINALTEMP.csv"
  },
  "mark": "rect",
  "encoding": {
    "x": {
